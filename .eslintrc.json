{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "react-app",
        "prettier"
    ],
    "plugins": [
        "react",
        "formatjs",
        "prettier"
    ],
    "rules": {
      "no-console": "warn",
      "formatjs/no-multiple-whitespaces": "warn",
      "formatjs/enforce-default-message": "error",
      "formatjs/enforce-placeholders": "error",
      "prettier/prettier": "error"
    }
}
